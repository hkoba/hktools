#!/bin/zsh

# [1] SVK/Git status notification

svkwatches=(
   /etc
)

# [2] permission enforcement

perm_ensure=(
   /var/log/httpd	0755
   /var/www/html        2775
)

# [3] ownership enforcement

owner_ensure=(
    /var/www/html      webmaster:devel
    /var/www/cgi-bin   webmaster:devel
)

# [4] should be removed(by rm) even after yum.

ensure_removed=(
)

# [5] rpm should be removed.

rpm_ensure_removed=(
)
